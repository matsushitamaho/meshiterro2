<div class="form" style="margin-left:200px;">
  <p style="font-size:50px;">プロフィール編集</p>
  <%= form_with model: @user do |f| %>
    <label for="inputName" style="padding-top:50px;">Name:</label>
    <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%><br>

    <label for="inputImage" style="padding-top:50px;">ProfileImage:</label>
    <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>

    <%= f.submit "変更を保存",style: "padding-top:50px;" %>
  <% end %>
</div>