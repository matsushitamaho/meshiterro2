<div class="container">
  <div class="row">
    <h3><%= @user.name %></h3>
  <%= image_tag @user.profile_image, width: 50, height: 50 %>
  <% if @user == current_user %>
    <p><%= link_to "プロフィール編集", edit_user_path %></p>
  <% end %>
  <%= render 'post_images/list', post_images: @post_images %>
  </div>
</div>
